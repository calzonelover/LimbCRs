@Article{ABBASI2005271,
title = "Observation of the ankle and evidence for a high-energy break in the cosmic ray spectrum",
journal = "Physics Letters B",
volume = "619",
number = "3",
pages = "271 - 280",
year = "2005",
issn = "0370-2693",
doi = "https://doi.org/10.1016/j.physletb.2005.05.064",
url = "http://www.sciencedirect.com/science/article/pii/S0370269305007525",
author = "R.U. Abbasi and T. Abu-Zayyad and J.F. Amman and G. Archbold and R. Atkins and J.A. Bellido and K. Belov and J.W. Belz and S.Y. [Ben Zvi] and D.R. Bergman and G.W. Burt and Z. Cao and R.W. Clay and B.C. Connolly and W. Deng and B.R. Dawson and Y. Fedorova and J. Findlay and C.B. Finley and W.F. Hanlon and C.M. Hoffman and G.A. Hughes and M.H. Holzscheiter and P. Hüntemeyer and C.C.H. Jui and K. Kim and M.A. Kirn and E.G. Loh and M.M. Maestas and N. Manago and L.J. Marek and K. Martens and J.A.J. Matthews and J.N. Matthews and A. O'Neill and C.A. Painter and L. Perera and K. Reil and R. Riehle and M. Roberts and M. Sasaki and S.R. Schnetzer and K.M. Simpson and G. Sinnis and J.D. Smith and R. Snow and P. Sokolsky and C. Song and R.W. Springer and B.T. Stokes and J.R. Thomas and S.B. Thomas and G.B. Thomson and D. Tupa and S. Westerhoff and L.R. Wiencke and A. Zech",
abstract = "We have measured the cosmic ray spectrum at energies above 1017eV using the two air fluorescence detectors of the High Resolution Fly's Eye experiment operating in monocular mode. We describe the detector, PMT and atmospheric calibrations, and the analysis techniques for the two detectors. We fit the spectrum to models describing galactic and extragalactic sources. Our measured spectrum gives an observation of a feature known as the “ankle” near 3×1018eV, and strong evidence for a suppression near 6×1019eV."
}

